{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    ["module-resolver", {
      "root": ["./src", "./test"],
      "alias": {
        "@aws": "./src/aws",
        "@commands": "./src/commands",
        "@prompts": "./src/prompts",
        "@transformers": "./src/transformers",
        "@utils": "./src/utils",
        "@validations": "./src/validations"
      }
    }]
  ],
  "ignore": ["./node_modules"]
}
